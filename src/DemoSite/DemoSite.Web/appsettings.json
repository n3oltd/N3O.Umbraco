{
  "$schema": "./appsettings-schema.json",
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information",
        "System": "Warning"
      }
    }
  },
  "AllowedHosts": "*",
  "Umbraco": {
    "CMS": {
      "Content": {
        "ContentVersionCleanupPolicy": {
          "EnableCleanup": true
        },
        "MacroErrors": "Throw"
      },
      "Global": {
        "Id": "8c75dc00-ab1d-4fda-b190-b3b8c586db7f"
      },
      "Hosting": {
        "Debug": false
      },
      "ModelsBuilder": {
        "FlagOutOfDateModels": false,
        "ModelsMode": "SourceCodeManual"
      },
      "RequestHandler": {
        "AddTrailingSlash": false,
        "ConvertUrlsToAscii": "try",
        "EnableDefaultCharReplacements": true,
        "UserDefinedCharCollection": [
          {
            "Char": " ",
            "Replacement": "-"
          },
          {
            "Char": "\\",
            "Replacement": ""
          },
          {
            "Char": "'",
            "Replacement": ""
          },
          {
            "Char": "%",
            "Replacement": ""
          },
          {
            "Char": ".",
            "Replacement": ""
          },
          {
            "Char": ";",
            "Replacement": ""
          },
          {
            "Char": "/",
            "Replacement": ""
          },
          {
            "Char": "\\\\",
            "Replacement": ""
          },
          {
            "Char": ":",
            "Replacement": ""
          },
          {
            "Char": "#",
            "Replacement": ""
          },
          {
            "Char": "+",
            "Replacement": "plus"
          },
          {
            "Char": "*",
            "Replacement": "star"
          },
          {
            "Char": "&",
            "Replacement": ""
          },
          {
            "Char": "?",
            "Replacement": ""
          },
          {
            "Char": "æ",
            "Replacement": "ae"
          },
          {
            "Char": "ä",
            "Replacement": "ae"
          },
          {
            "Char": "ø",
            "Replacement": "oe"
          },
          {
            "Char": "ö",
            "Replacement": "oe"
          },
          {
            "Char": "å",
            "Replacement": "aa"
          },
          {
            "Char": "ü",
            "Replacement": "ue"
          },
          {
            "Char": "ß",
            "Replacement": "ss"
          },
          {
            "Char": "|",
            "Replacement": "-"
          },
          {
            "Char": "<",
            "Replacement": ""
          },
          {
            "Char": ">",
            "Replacement": ""
          }
        ]
      },
      "RuntimeMinification": {
        "useInMemoryCache": true,
        "cacheBuster": "Timestamp"
      }
    }
  },
  "uSync": {
    "Settings": {
      "ImportAtStartup": "None",
      "ExportAtStartup": "None",
      "ExportOnSave": "None"
    }
  }
}
