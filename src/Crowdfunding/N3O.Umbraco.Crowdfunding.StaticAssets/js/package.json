    "@uppy/progress-bar": "^4.0.0",
    "@uppy/react": "^4.0.1",
    "@uppy/xhr-upload": "^4.0.2",
    "ahooks": "^3.8.0",
    "ckeditor5": "^42.0.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
