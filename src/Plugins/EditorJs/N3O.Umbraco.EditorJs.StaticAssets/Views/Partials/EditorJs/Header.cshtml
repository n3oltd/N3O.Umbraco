@model N3O.Umbraco.EditorJs.Models.EditorJsBlock<N3O.Umbraco.EditorJs.HeaderBlockData, N3O.Umbraco.EditorJs.HeaderTunesData>

@switch (Model.Data.Level) {
	case 1:
		<h1 style="text-align: @Model.Tunes?.AlignmentTune?.Alignment">@Html.Raw(Model.Data.Text)</h1>
		break;

	case 2:
		<h2 style="text-align: @Model.Tunes?.AlignmentTune?.Alignment">@Html.Raw(Model.Data.Text)</h2>
		break;

	case 3:
		<h3 style="text-align: @Model.Tunes?.AlignmentTune?.Alignment">@Html.Raw(Model.Data.Text)</h3>
		break;

	case 4:
		<h4 style="text-align: @Model.Tunes?.AlignmentTune?.Alignment">@Html.Raw(Model.Data.Text)</h4>
		break;

	case 5:
		<h5 style="text-align: @Model.Tunes?.AlignmentTune?.Alignment">@Html.Raw(Model.Data.Text)</h5>
		break;

	case 6:
		<h6 style="text-align: @Model.Tunes?.AlignmentTune?.Alignment">@Html.Raw(Model.Data.Text)</h6>
		break;
}